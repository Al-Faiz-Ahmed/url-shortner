generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model short_urls {
  id String @id @default(uuid())
  givenURL String @map("given_url")
  generatedURL String @map("generated_url")
  uniqueHash String @map("unique_hash")

  // Users id 

  user_id has to be added as foreign key  

  @@map("short_urls")
}